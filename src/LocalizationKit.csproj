<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <RootNamespace>AluminiumTech.DevKit.LocalizationKit</RootNamespace>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>LocalizationKit</Title>
        <Authors>AluminiumTech</Authors>
        <Description>Make Localizations in .NET easy and less painful. </Description>
        <Copyright>Copyright (c) 2021</Copyright>
        <PackageLicenseUrl></PackageLicenseUrl>
        <RepositoryUrl>https://gitlab.com/aluminiumtechdevkit/LocalizationKit</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageReleaseNotes>### Changes
* Updated SettingsKit from 2.0.0 to 2.0.1</PackageReleaseNotes>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <AssemblyVersion>2.0.0.0</AssemblyVersion>
        <FileVersion>2.0.0.0</FileVersion>
        <Version>2.0.0</Version>
        <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
        <PackageVersion>2.0.1</PackageVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="SettingsKit" Version="2.0.1" />
    </ItemGroup>

    <ItemGroup>
      <None Include="..\LICENSE.md">
        <Pack>True</Pack>
        <PackagePath></PackagePath>
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
    </ItemGroup>

</Project>
